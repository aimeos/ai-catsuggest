{
	"name": "aimeos/ai-catsuggest",
	"description": "Aimeos category suggestion in catalog/suggest HTML client",
	"keywords": ["aimeos", "extension"],
	"homepage": "https://aimeos.org",
	"type": "aimeos-extension",
	"license": "LGPL-3.0-or-later",
	"support": {
		"source": "https://github.com/aimeos/ai-catsuggest",
		"issues": "https://github.com/aimeos/ai-catsuggest/issues",
		"forum": "https://aimeos.org/help",
		"wiki": "https://aimeos.org/docs"
	},
	"prefer-stable": true,
	"minimum-stability": "dev",
	"require": {
		"php": "^8.0.11",
		"aimeos/aimeos-core": "dev-master",
		"aimeos/ai-controller-frontend": "dev-master"
	},
	"autoload": {
		"psr-4": {
			"Aimeos\\": "src"
		},
		"classmap": [
			"src"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"Aimeos\\": "tests"
		},
		"classmap": [
			"tests"
		]
	}
}